<!--Secoes de venda-->

<main>
    <!--Bones-->
    <div class="container" id="vitrine-bone">
        <div class="row">
            {{#each produto}}
            <div class="col-sm-4 col-md-3">
                <div class="card" id="produto-background">
                <img src="http://localhost:8080/public/images/logo.png"{{dataValues.imagemBone}} class="card-img">
                <div class="card-body">
                    <h5 class="card-title">{{dataValues.marcaBone}}</h5>
                    <p class="card-text text-dark">{{dataValues.valorBone}}</p>
                </div> 
                </div>
                <hr id="linha">
            </div> 
            {{/each}}               
        </div>
    </div>
 
 <!--DAQUI PRA BAIXO AINDA VAI SER FEITO-->
    <!--Carteiras-->
    <div class="container">
        <div class="container" id="vitrine-carteiras">
            <div class="card-deck">
                <div class="card" id="produto-background">
                    <img src="http://localhost:8080/public/images/logo.png" class="card-img-top" alt="...">
                    <div class="card-body">
                        <h5 class="card-title">{{tituloCarteira}}</h5>
                        <p class="card-text">{{descricaoCarteira}}</p>
                        <p class="card-text"><small class="text-dark">{{unidadeCarteira}}</small></p>
                    </div>
                </div>
                <hr id="linha">
                
                <hr id="linha">
                
            </div>
        </div>
    </div>

    <!--Camisas-->
    <div class="container">
        <div class="container" id="vitrine-camisas">
            <div class="card-deck">
                <div class="card" id="produto-background">
                    <img src="http://localhost:8080/public/images/logo.png" class="card-img-top" alt="...">
                    <div class="card-body">
                        <h5 class="card-title">{{a}}</h5>
                        <p class="card-text">{{descricaoCamisa}}</p>
                        <p class="card-text"><small class="text-dark">{{unidadeCamisa}}</small></p>
                    </div>
                </div>
                <hr id="linha">
                <hr id="linha">
            </div>
        </div>
    </div>
</main>

